<TfrxReport Version="4.9.38" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Padrão" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="42503,6890770602" ReportOptions.Description.Text="" ReportOptions.LastChange="44983,8091141898" ScriptLanguage="PascalScript" ScriptText.Text="var FDebito, FCredito : Extended;&#13;&#10;    s_usu : String;&#13;&#10;    MesCorrente : String;                                                  &#13;&#10;const&#13;&#10;  SQL_RELATORIO =&#13;&#10;' SELECT FIN_DESCRICAO,FIN_VALORLIMITE,VALOR_PARCELAS,                        '#13+&#13;&#10;'        (FIN_VALORLIMITE - VALOR_PARCELAS) SALDO_LIMITE,                     '#13+&#13;&#10;'        FIN_MELHORDIA, FIN_VENCTODIA                                         '#13+&#13;&#10;'   FROM (select coalesce(                                                    '#13+&#13;&#10;'                (select sum(case when(coalesce(parcelas.par_valor,0)=0) then '#13+&#13;&#10;'                               parcelas.par_valor_pago                       '#13+&#13;&#10;'                             else                                            '#13+&#13;&#10;'                               parcelas.par_valor                            '#13+&#13;&#10;'                             end)                                            '#13+&#13;&#10;'                    from parcelas                                            '#13+&#13;&#10;'                   where parcelas.par_fin_id=financeiro.fin_id               '#13+&#13;&#10;'                     and extract(month from parcelas.par_vencto) &#62;= %s       '#13+&#13;&#10;'                     and extract(year from parcelas.par_vencto) &#62;= %s        '#13+&#13;&#10;'                  ) ,0) valor_parcelas,                                      '#13+&#13;&#10;'                financeiro.FIN_DESCRICAO,                                    '#13+&#13;&#10;'                (SELECT N_LIMITE FROM PR_RETORNA_LIMITE(financeiro.FIN_ID)) FIN_VALORLIMITE, '#13+&#13;&#10;'                financeiro.FIN_MELHORDIA,                                    '#13+&#13;&#10;'                financeiro.FIN_VENCTODIA                                     '#13+&#13;&#10;'           from financeiro                                                   '#13+&#13;&#10;'          where 1=1                                                          '#13+&#13;&#10;'            and financeiro.fin_inativo = ''N''                               '#13+&#13;&#10;'            and financeiro.fin_debcred = ''D''                               '#13+&#13;&#10;'            and exists (select usuario_visao.UVIS_USU_FILHO                  '#13+&#13;&#10;'                          from usuario_visao                                 '#13+&#13;&#10;'                         where usuario_visao.UVIS_USU_PAI = %s               '#13+&#13;&#10;'                           and usuario_visao.UVIS_USU_FILHO =                '#13+&#13;&#10;'                           financeiro.fin_usu_id )                           '#13+&#13;&#10;'          order by financeiro.fin_descricao)                                 '#13+&#13;&#10;'  where FIN_VALORLIMITE&#62;0                                                    ';&#13;&#10;  &#13;&#10;&#13;&#10;   &#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  edAno.Text := IntToStr(YearOf(Now));&#13;&#10;  edAno.SetFocus;&#13;&#10;  cbMes.ItemIndex := (MonthOf(Now)-1);                                            &#13;&#10;end;&#13;&#10;&#13;&#10;procedure BitBtn1OnClick(Sender: TfrxComponent);&#13;&#10;var s_debcred : String;&#13;&#10;    i_usu : Integer;&#13;&#10;    sFiltro : String;                                            &#13;&#10;&#13;&#10;begin&#13;&#10;  s_debcred := 'D';                                                     &#13;&#10;  i_usu := StrToInt(&#60;usuario&#62;);        &#13;&#10;          &#13;&#10;  IBXQuery1.Close;&#13;&#10;  IBXQuery1.SQL.Clear;                                    &#13;&#10;  IBXQuery1.SQL.Text := Format(SQL_RELATORIO,[IntToStr(cbMes.ItemIndex+1),edAno.Text,IntToStr(i_usu)]);&#13;&#10;  //ShowMessage(IBXQuery1.SQL.Text);    &#13;&#10;  IBXQuery1.open;&#13;&#10;  sFiltro := 'Mês: '+cbMes.Text+' | ';                                        &#13;&#10;  sFiltro := sFiltro + 'Ano: '+ edAno.Text+' ';                                                         &#13;&#10;&#13;&#10;  s_usu := &#60;nomeusuario&#62;;        &#13;&#10;&#13;&#10;  sFiltro := sFiltro + ' | Usuário: ' + s_usu;&#13;&#10;  Memo31.Text := sFiltro;&#13;&#10;    &#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupBox2OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;end." PropData="">
